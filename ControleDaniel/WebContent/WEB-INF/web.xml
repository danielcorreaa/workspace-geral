  </servlet>
  <servlet-mapping>
  	    <servlet-name>EstabelecimentoController</servlet-name>
  	    <url-pattern>/estabelecimento.do</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
		<servlet-name>ClienteAction</servlet-name>
		<servlet-class>com.controle.controller.ClienteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ClienteAction</servlet-name>
		<url-pattern>/clienteAction</url-pattern>
	</servlet-mapping>
  
</web-app>